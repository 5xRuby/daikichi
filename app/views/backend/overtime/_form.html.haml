= simple_form_for current_object, url: backend_overtime_path(current_object), method: :put do |f|
  = f.button :submit, t("title.backend/overtime.approve"), disabled: !current_object.may_approve?, name: "approve", class: "btn btn-primary"
  = f.button :submit, t("title.backend/overtime.reject"), name: "reject", class: "btn btn-danger"

